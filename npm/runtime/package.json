{
  "name": "@opal/runtime",
  "description": "Opal Runtime",
  "keywords": [
    "javascript",
    "opal",
    "runtime",
    "ruby"
  ],
  "author": "Guillaume Grossetie",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "files": [
    "src"
  ],
  "directories": {
    "lib": "src"
  },
  "type": "module",
  "main": "./src/index.cjs",
  "browser": "./src/index.mjs",
  "exports": {
    "node": {
      "import": "./src/index.js",
      "require": "./src/index.cjs"
    },
    "default": "./src/index.mjs"
  },
  "scripts": {
    "build": "rollup --config",
    "test": "node --test",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/opal/opal",
  "bugs": "https://github.com/opal/opal/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/opal/opal.git"
  },
  "dependencies": {
    "glob": "10.4.2",
    "unxhr": "1.2.0"
  },
  "devDependencies": {
    "rollup": "~3.29",
    "rollup-plugin-re": "1.0.7"
  }
}
